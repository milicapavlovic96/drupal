<form action="/productlists" method="get">
<p>Search by title:</p>
<input type="text" name="token" value="" placeholder="Search">
<input type="submit" value="submit" />
</form>
<br/>
<br/>
{% for product in items %}
    
     <h3>{{ product.title }}</h3>
     
     <h5>Description:</h5> {{ product.description | raw}}
    
     <img class="top-article-image" src="{{ file_url(product.image) }}"/>
     
     <hr>
{% endfor %}
    
